{
  "name": "template",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@fullhuman/postcss-purgecss": "^2.1.0",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "autoprefixer": "^9.7.4",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^7.1.0",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-scripts": "3.4.0",
    "tailwindcss": "^1.2.0",
    "tailwindcss-bg-alpha": "^2.1.0"
  },
  "scripts": {
    "start": "npm-run-all -s start:js start:css",
    "build": "npm-run-all build:css build:js",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "start:js": "react-scripts start",
    "build:js": "react-scripts build",
    "start:css": "postcss src/css/tailwind.src.css -o src/css/tailwind.css -w",
    "build:css": "postcss src/css/tailwind.src.css -o src/css/tailwind.css --env production"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
